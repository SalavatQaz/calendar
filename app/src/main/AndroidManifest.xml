<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.calendarv3" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CalendarV3"
        android:usesCleartextTraffic="true" >
        <activity
            android:name=".Pages.CreateNewEvent"
            android:exported="false" />
        <activity
            android:name=".Pages.MainPage"
            android:exported="false" />
        <activity
            android:name=".Pages.SignUp"
            android:exported="false" />
        <activity
            android:name=".Pages.Login"
            android:exported="false" />
        <activity
            android:name=".Pages.MainActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>